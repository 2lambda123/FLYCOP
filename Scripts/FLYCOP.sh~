#!/bin/bash

############ FLYCOP ############
# Author: Beatriz García-Jiménez
# April 2018
################################

# Call: FLYCOP.sh 'synKtPHA' 1 V0 'MaxPHA'

domainName=$1
id=$2 # '20', '21', ...
templateID=$3 # 'V0', 'V1', 'V2', 'V5' ...
fitness=$4 # 'MaxGR', 'MaxYield'
numOfRuns=$5 # 500, 10

smac --scenario-file ../Scripts/${domainName}_confFLYCOP_scenario_v${id}.txt --validation false --numberOfRunsLimit ${numOfRuns}
sh ../Scripts/FLYCOPanalyzingResults_${domainName}.sh ${id} ${templateID} $fitness ${numOfRuns}



